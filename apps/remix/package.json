{
  "name": "remix",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "tailwindcss -m -o ./app/styles/tailwind.css",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "tailwindcss -w -o ./app/styles/tailwind.css",
    "dev:remix": "remix dev",
    "dev:ladle": "ladle serve",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "^1.6.0",
    "@remix-run/react": "^1.6.0",
    "@remix-run/serve": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ladle/react": "^1.2.0",
    "@remix-run/dev": "^1.6.0",
    "@remix-run/eslint-config": "^1.6.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "api-contract": "*",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
